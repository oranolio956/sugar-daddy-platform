{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NODE_VERSION": "18.x",
    "NEXT_TELEMETRY_DISABLED": "1",
    "NPM_CONFIG_PREFIX": "/tmp/npm",
    "NPM_CONFIG_CACHE": "/tmp/npm-cache",
    "NPM_CONFIG_STRICT_SSL": "false",
    "NPM_CONFIG_ENGINE_STRICT": "false",
    "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
  },
  "functions": {
    "src/pages/api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  }
}